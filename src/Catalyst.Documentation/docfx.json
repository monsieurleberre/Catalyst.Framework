  "build": {
    "content": [
      {
        "files": [
          "api/**/**.yml",
          "api/**/**.md",
          "api/toc.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/toc.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© 2019 <a href='https://catalystnet.org'>Catalyst Network</a>",
      "_appTitle": "Catalyst Framework Documentation",
      "_enableSearch": "true",
      "_appLogoPath": "images/logo.svg",
      "_gitContribute": {
        "repo": "https://github.com/catalyst-network/catalyst.node"
      },
      "_gitUrlPattern": "github"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
